{
  "rewrites": [
    {
      "source": "/api/mantis/(.*)",
      "destination": "https://mantis.xcelis.com.br/mantis/api/rest/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/mantis/(.*)",
      "headers": [
        {
          "key": "Authorization",
          "value": "GjDoN1uca9IC6GiZUcidMS5aixv6d8lZ"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Accept",
          "value": "application/json"
        },
        {
          "key": "X-Requested-With",
          "value": "XMLHttpRequest"
        }
      ]
    }
  ],
  "cors": {
    "allowedOrigins": ["*"],
    "allowedMethods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
    "allowedHeaders": ["Authorization", "Content-Type", "Accept", "X-Requested-With"]
  }
}